{
  "name": "mrthito/laravel-stripe-connect",
  "type": "library",
  "keywords": [
    "laravel",
    "stripe",
    "billing"
  ],
  "require": {
    "illuminate/database": "^10.0|^11.0|^12.0",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "stripe/stripe-php": ">=10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "MrThito\\LaravelStripeConnect\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MrThito\\LaravelStripeConnect\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MrThito\\LaravelStripeConnect\\ServiceProvider"
      ]
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "rap2h",
      "email": "raphaelht@gmail.com"
    },
    {
      "name": "Simon Hamp",
      "email": "simon.hamp@me.com"
    },
